<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-autowire="byName">
	
	<!-- 客户端配置 -->
	<bean id="exampleWSJava" class="com.dianping.dpsf.spring.ProxyBeanFactory" init-method="init">
		<property name="serviceName" value="http://api.sys.local.dp/Service/AdminService.svc?wsdl"/>
		<property name="iface" value="org.tempuri.IAdminService"/>
		<!-- default:pb -->
		<property name="serialize" value="ws"/>
		<!-- 单位毫秒 default:2000 -->
		<property name="timeout" value="2000"/>
	</bean>
	
</beans>