<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans default-autowire="byName">

    <!-- 多个服务注册管理器  分别监听20000（默认）和20001端口 -->
	<bean id="exampleService" class="com.dianping.dpsf.spring.ServiceRegistry" init-method="init" lazy-init="false">
		<property name="services">
			<map>
				<entry key="http://service.dianping.com/itemName/exampleService_1.0.0" value-ref="exampleImpl"/>
				<entry key="http://service.dianping.com/itemName/javaExampleService_1.0.0" value-ref="exampleImplJava"/>
				<entry key="http://service.dianping.com/itemName/thriftExampleService_1.0.0" value-ref="exampleImplThrift"/>
			</map>
		</property>
	</bean>
		<bean id="exampleService1" class="com.dianping.dpsf.spring.ServiceRegistry" init-method="init" lazy-init="false">
		<property name="services">
			<map>
				<entry key="http://service.dianping.com/itemName/exampleService_1.0.0" value-ref="exampleImpl"/>
				<entry key="http://service.dianping.com/itemName/javaExampleService_1.0.0" value-ref="exampleImplJava1"/>
				<entry key="http://service.dianping.com/itemName/thriftExampleService_1.0.0" value-ref="exampleImplThrift"/>
			</map>
		</property>
		<property name="port" value="20001"/>
		
	</bean>
	
	<bean id="exampleImpl" class="com.dianping.dpsf.example.protobuf.ExampleServiceImpl"/>
	
	
	
	<bean id="exampleImplJava1" class="com.dianping.dpsf.example.java.JavaExampleServiceImpl">
		<property name="dd" value="2222222222222"/>
	</bean>
	<bean id="exampleImplJava" class="com.dianping.dpsf.example.java.JavaExampleServiceImpl">
		<property name="dd" value="1111111111111"/>
	</bean>
	
	<bean id="exampleImplThrift" class="com.dianping.dpsf.example.thrift.ExampleServiceImpl"/>
	
	<!-- 自定义telnet命令实际执行实现 -->
	<bean id="telnetTest" class="com.dianping.dpsf.example.TelnetCmdTest"/>
	<bean id="telnetTest1" class="com.dianping.dpsf.example.TelnetCmdTest1"/>
	
	<!-- 自定义telnet监控命令入口 -->
	<bean id="telnetCmdCustom" class="com.dianping.dpsf.telnet.cmd.TelnetCommandCustom" init-method="init">
		<property name="cmds">
			<map>
				<!--key=cmd   value = beanId @ methodName @ description -->
				<entry key="dd" value="telnetTest@dd@dd command description"/>
				<entry key="ss" value="telnetTest@ss@ss command description"/>
			</map>
		</property>
		<!-- 自定义用户名和密码 -->
		<property name="username" value="test"/>
		<property name="password" value="12qwaszx"/>
	</bean>
</beans>